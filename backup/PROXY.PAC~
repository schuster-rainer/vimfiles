  
function FindProxyForURL(url,host)
{
    if(isPlainHostName(host) || isInNet(dnsResolve(host),"192.168.11.0","255.255.248.0")) {
        return "DIRECT";
    } else if (isInNet(myIpAddress(),"192.168.11.0","255.255.248.0")) {
        return "PROXY proxy.clondiag.jena:8080";
    } else {
        return "PROXY proxy.clondiag.jena:8080; DIRECT";
    }
}
